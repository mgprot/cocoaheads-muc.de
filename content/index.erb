<h2>Nächstes Treffen:</h2>
<% sorted_posts_by_date[0,1].each do |post| %>
    <div class="post-list">
        <p><%= post[:date].to_date_time(:human) %>:</p>
        <h2><%= link_to post[:title], post.path %></h2>
        <%= post.compiled_content %>
    </div>
<% end %>
<p style="margin-top:5em"><a href="/archive">&rarr; Ältere Vorträge</a></p>